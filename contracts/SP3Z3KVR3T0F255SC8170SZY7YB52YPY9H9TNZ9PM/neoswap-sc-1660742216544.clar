		(asserts! (is-ok (contract-call? 'SP2A665S3H6FVMZSY4VJ17ESXX21CGS0A32984B1H.Punks-Army-Upgrade-NFTs transfer u344 tx-sender (as-contract tx-sender))) cannot-escrow-nft )
		(var-set agent-6-status true)
