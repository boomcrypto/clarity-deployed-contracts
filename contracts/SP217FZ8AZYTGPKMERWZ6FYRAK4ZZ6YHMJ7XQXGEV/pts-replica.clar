(define-constant ERR-NOT-AUTHORIZED (err u704))
(define-constant admin tx-sender)
(define-map id-checked uint bool)
(define-data-var shutoff-valve bool false)
(define-read-only (get-id-checked (id uint)) (default-to false (map-get? id-checked id)))

(define-public (points-replica (ids (list 1000 uint)))
    (ok (map point-replica ids))
)

(define-private (point-replica (id uint)) 
    (let (
        (balance (contract-call? 'SP1N057R0S5BBAQKTW0GF2J7BXKFHY2GQV9MP2BEN.spoints get-balance id))
    )
    (asserts! (is-eq (var-get shutoff-valve) false) ERR-NOT-AUTHORIZED) 
    (asserts! (is-eq tx-sender admin) ERR-NOT-AUTHORIZED)
    (if (and (> balance u0) (not (get-id-checked id))) 
        (begin
            (try! (contract-call? .spoints admin-airdrop id balance)) 
            (map-set id-checked id true)
            (ok true)
        ) 
        (ok true))))

(contract-call? .spoints principal-approve (as-contract tx-sender))
