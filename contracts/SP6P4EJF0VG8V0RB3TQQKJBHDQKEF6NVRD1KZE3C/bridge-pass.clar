;; Satoshibles Bridge Pass
;; One-time use free bridge pass for ids that already paid escrow before

(define-constant ERR-NOT-AUTHORIZED (err u410))
(define-map bridge-pass uint bool)

;; Public
(define-public (bridge-pass-set-used (id uint))
    (begin
        (asserts! (is-eq contract-caller .stacksbridge-satoshibles-v2) ERR-NOT-AUTHORIZED)
        (map-set bridge-pass id true)
        (ok true)
    )
)

;; Read Only
(define-read-only (bridge-pass-is-used (id uint))
  (default-to true (map-get? bridge-pass id))
)

